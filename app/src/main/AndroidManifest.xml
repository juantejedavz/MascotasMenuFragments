<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mx.blogspot.juanmtejedav.mascotasmenufragments">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainMascotas">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="mx.blogspot.juanmtejedav.mascotasmenufragments.ExtraActivities.AcercaDeActivity"
            android:parentActivityName=".MainMascotas"/>
        <activity android:name="mx.blogspot.juanmtejedav.mascotasmenufragments.ExtraActivities.ContactoActivity"
            android:parentActivityName=".MainMascotas"/>
        <activity android:name="mx.blogspot.juanmtejedav.mascotasmenufragments.MascotasFav"
            android:parentActivityName=".MainMascotas"></activity>


    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>